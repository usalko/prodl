{{ block "digraph" }}digraph model_graph {
  // Dotfile by prodl (https://github.com/usalko/prodl)
  // Created: {{ .CreatedAt }}
  {{ if .CliOptions }}// Cli Options: {{ .CliOptions }}{{ end }}

  {{ block "digraph_options" }}fontname = "Helvetica"
  fontsize = 8
  splines  = true
  rankdir = "{{ .Rankdir }}"{{ end }}

  node [{{ block "node_options" }}
    fontname = "Helvetica"
    fontsize = 8
    shape = "plaintext"
  {{ end }}]

  edge [{{ block "edge_options" }}
    fontname = "Helvetica"
    fontsize = 8
  {{ end }}]

  // Labels
{{ block "labels" }}{{ range .Graphs }}{{ template "label.dot" }}{{ end }}{{ end }}

  // Relations
{{ block "relations" }}{{ for .Graphs }}{{ template "relation.dot" }}{{ end }}{{ end }}
}{{ end }}
